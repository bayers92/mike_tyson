<div class = "row edit-page">
	<%= render 'application/sideNav'%>
	<div class = "small-12 medium-9 columns form-yield">
		<h4 class = "header"> Edit Showcase Information </h4>
		<%= render 'form' %>
		<br/>
		<br/>
		<br/>


		<% if current_user.showcase.showcase_type != nil %>
			<%= button_to "Add New Project", new_project_path, :class => "button", :method => :get %>
		<% end %>

		<%= render @showcase.projects.reverse %>
		<br/>
	</div>
</div>