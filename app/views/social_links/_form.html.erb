<%= simple_form_for(@social_link) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  	<%= f.input :link_alt_email %>
  	<%= f.input :link_linkedin %>
  	<%= f.input :link_instagram %>
  	<%= f.input :link_facebook %>
  	<%= f.input :link_twitter %>
  	<%= f.input :link_tumblr %>
  	<%= f.input :link_github %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
