<%= simple_form_for(@school) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  	<%= f.input :name %>
  	<%= f.input :logo %>
  	<%= f.input :color1 %>
  	<%= f.input :color2 %>
    <%= f.input :domain %>
  </div>

  <div class="form-actions">
   <%= f.button :submit, 'Save' %>
  </div>
<% end %>
