<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  	<%= f.input :title %>
  	<%= f.input :paragraph %>
  	<%= f.input :file1 %>
  	<%= f.input :file2 %>
  	<%= f.input :file3 %>
  	<%= f.input :file4 %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
