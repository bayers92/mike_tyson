<%= simple_form_for(@photo_link) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tumblr_url, label: "Name of Tumblr Blog:", placeholder: "my-awesome-photo-blog" %>
  </div>

  <div class="form-actions">
   <%= f.button :submit, 'Save' %>
   <a href="#" data-reveal-id="previewModal" class = "button alt3">Preview</a>
  </div>
<% end %>
